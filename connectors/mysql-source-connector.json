{
  "name": "mysql-source-connector",
  "config": {
    "connector.class": "io.debezium.connector.mysql.MySqlConnector",
    "tasks.max": "1",
    "database.hostname": "mysql",
    "database.port": "3306",
    "database.user": "debezium",
    "database.password": "dbz",
    "database.server.id": "184054",
    "database.server.name": "mysql",
    "database.include.list": "legacy_db",
    "database.history.kafka.bootstrap.servers": "kafka:9092",
    "database.history.kafka.topic": "dbhistory.mysql",
    "include.schema.changes": "true",
    "transforms": "unwrap,AddPrefix,AddMetadata",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.AddPrefix.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.AddPrefix.regex": "mysql.legacy_db.(.*)",
    "transforms.AddPrefix.replacement": "mysql.sync.$1",
    "transforms.AddMetadata.type": "org.apache.kafka.connect.transforms.InsertField$Value",
    "transforms.AddMetadata.static.field": "sync_source",
    "transforms.AddMetadata.static.value": "mysql",
    "decimal.handling.mode": "double",
    "time.precision.mode": "adaptive",
    "snapshot.mode": "schema_only",
    "snapshot.locking.mode": "none",
    "binlog.buffer.size": "0",
    "poll.interval.ms": "1000",
    "max.batch.size": "2048",
    "max.queue.size": "8192",
    "heartbeat.interval.ms": "10000",
    "slot.drop.on.stop": "false",
    "signal.data.collection": "legacy_db.debezium_signal",
    "topic.prefix": "mysql"
  }
}